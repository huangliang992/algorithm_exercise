package recrusion;

public class ¾ØÐÎ¸²¸Ç {
	public int RectCover(int target) {
        int total = 0;
        for(int i=0; i<=target; i++){
            if(i%2==0){
                if(i/2>(target-i+1) || i==0){
                    total = total +1;
                }else{
                    total = total + chooseMFromN((target-i+1), i/2);
                }
            }
        }
        return total;
    }
    
    public int chooseMFromN(int n, int m){
        int a1=1;
        for(int i=0; i<m;i++){
            a1 = a1 * n;
            n--;
        }
        int a2= 1;
        for(int i=m; i>0;m--){
            a2=a2*m;
        }
        return a1/a2;
    }
    
    public static void main(String args[]){
    	¾ØÐÎ¸²¸Ç a = new ¾ØÐÎ¸²¸Ç();
    	System.out.println(a.RectCover(4));
    }
}
