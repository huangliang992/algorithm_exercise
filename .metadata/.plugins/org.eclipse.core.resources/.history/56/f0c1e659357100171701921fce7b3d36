package jingdong;

import java.util.Scanner;

public class Main10 {
	
	public static void main(String args[]){
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		int [] a = new int[n];
		a[0]=1;
		for(int i=1;i<n;i++){
			a[i] = 0;
		}
		int num = 0;
		count(n, 2, num, a);
		System.out.println(num);
	}

	public static void count(int n, int t, int num, int[] a){
		if(t<n){
			for(int i=0;i<2;i++){
				boolean f = judge(a, t, i);
				if(f){
					a[t] = i;
					count(n,t+1,num,a);
				}
			}
		}else{
			num++;
		}
	}
	
	/**
	 * 第j层能否放i(1y0n),j从1开始
	 * @param a
	 * @param j
	 * @return
	 */
	public static boolean judge(int [] a, int j, int i){
		boolean f = true;
		for(int k=1;k<a.length;k++){
			if(j%(k+1)==0 && a[k] ==0){
				return false;
			}
		}
		return f;
	}
}
